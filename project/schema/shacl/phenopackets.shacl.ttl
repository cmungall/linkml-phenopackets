            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path <disease:resolution> ],
        [ sh:class <https://w3id.org/linkml/phenopackets/base/OntologyClass> ;
            sh:description "The identifier of this disease e.g. MONDO:0007043, OMIM:101600, Orphanet:710, DOID:14705 (note these are all equivalent) ARGO mapping primary_diagnosis::submitter_primary_diagnosis_id" ;
