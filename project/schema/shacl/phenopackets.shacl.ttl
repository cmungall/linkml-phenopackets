        [ sh:class <https://w3id.org/linkml/phenopackets/base/OntologyClass> ;
            sh:description "Disease staging, the extent to which a disease has developed. For cancers, see https://www.cancer.gov/about-cancer/diagnosis-staging/staging Valid values include child terms of NCIT:C28108 (Disease Stage Qualifier) ARGO mapping primary_diagnosis::clinical_tumour_staging_system ARGO mapping primary_diagnosis::clinical_stage_group" ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path <disease:diseaseStage> ],
        [ sh:class <https://w3id.org/linkml/phenopackets/base/TimeElement> ;
